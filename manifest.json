{
  "description": "PHP obj inj detect",
  "manifest_version": 2,
  "name": "PHP obj inj detect",
  "version": "1.0",

  "applications": {
    "gecko": {
      "id": "php-object-injection-detect@mozilla.org",
      "strict_min_version": "53.0"
    }
  },

  "permissions": [
    "webRequest",
    "<all_urls>",
    "cookies",
    "storage"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "Info",
    "default_popup": "popup/popup.html",
    "browser_style": true
  }
}
