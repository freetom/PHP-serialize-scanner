{
  "description": "Reveal potential POI vulnerabilities by analyzing attack vectors (GET,POST,COOKIES)",
  "manifest_version": 2,
  "name": "PHP obj inj detect",
  "version": "0.1",

  "applications": {
    "gecko": {
      "id": "PHP-serialize-scanner@freetom.me",
      "strict_min_version": "53.0"
    }
  },

  "permissions": [
    "webRequest",
    "<all_urls>",
    "cookies",
    "storage"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "Info",
    "default_popup": "popup/popup.html",
    "browser_style": true
  }
}
